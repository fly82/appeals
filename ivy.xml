<?xml version="1.0"?>
<ivy-module version="1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="Mukha" module="appeals"/>

  <configurations>
    <conf name="default" description="for runtime"/>
    <conf name="compile"/>
    <conf name="test"/>
    <conf name="devel"/>
  </configurations>

  <dependencies defaultconf="default->default">
      <dependency org="com.h2database" name="h2" rev="1.3+"/>
      <dependency org="com.google.guava" name="guava" rev="15.0"/>
      <dependency org="org.json" name="json" rev="20131018"/>
      <dependency org="net.sourceforge.jtds" name="jtds" rev="1.3.1"/>

      <dependency org="junit" name="junit" rev="4.8+" conf="test->default"/>
      <dependency org="org.mockito" name="mockito-all" rev="1.8.5" conf="test->default"/>

  </dependencies>
</ivy-module>